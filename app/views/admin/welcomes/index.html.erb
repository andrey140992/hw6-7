<p id="notice"><%= notice %></p>

<h1>Admin Welcomes</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_welcomes.each do |admin_welcome| %>
      <tr>
        <td><%= admin_welcome.index %></td>
        <td><%= link_to 'Show', admin_welcome %></td>
        <td><%= link_to 'Edit', edit_admin_welcome_path(admin_welcome) %></td>
        <td><%= link_to 'Destroy', admin_welcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Welcome', new_admin_welcome_path %>
